        System.out.print("Enter any year = ");\par
        int year = s.nextInt();\par
        boolean flag = false;\par
        if(year % 400 == 0)\par
        \{\par
            flag = true;\par
        \}\par
        else if (year % 100 == 0)\par
        \{\par
            flag = false;\par
        \}\par
        else if(year % 4 == 0)\par
        \{\par
            flag = true;\par
        \}\par
        else\par
        \{\par
            flag = false;\par
        \}\par
        if(flag)\par
        \{\par
            System.out.println("Year "+year+" is a Leap Year");\par
        \}\par
        else\par
        \{\par
            System.out.println("Year "+year+" is not a Leap Year");\par
        \}\par
    \}\par
\}\par
