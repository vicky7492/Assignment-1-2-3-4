\{\par
    public static void main(String args[] )\par
    \{ \par
        int balance = 5000, withdraw, deposit;\par
        Scanner s = new Scanner(System.in);\par
        while(true)\par
        \{\par
            System.out.println("Automated Teller Machine");\par
            System.out.println("Choose 1 for Withdraw");\par
            System.out.println("Choose 2 for Deposit");\par
            System.out.println("Choose 3 for Check Balance");\par
            System.out.println("Choose 4 for EXIT");\par
            System.out.print("Choose the operation you want to perform:");\par
            int n = s.nextInt();\par
            switch(n)\par
            \{\par
                case 1:\par
                System.out.print("Enter money to be withdrawn:");\par
                withdraw = s.nextInt();\par
                if(balance >= withdraw)\par
                \{\par
                    balance = balance - withdraw;\par
                    System.out.println("Please collect your money");\par
                \}\par
                else\par
                \{\par
                    System.out.println("Insufficient Balance");\par
                \}\par
                System.out.println("");\par
                break;\par
 \par
                case 2:\par
                System.out.print("Enter money to be deposited:");\par
                deposit = s.nextInt();\par
                balance = balance + deposit;\par
                System.out.println("Your Money has been successfully depsited");\par
                System.out.println("");\par
                break;\par
 \par
                case 3:\par
                System.out.println("Balance : "+balance);\par
                System.out.println("");\par
                break;\par
 \par
                case 4:\par
                System.exit(0);\par
            \}\par
        \}\par
    \}\par
\}\par
